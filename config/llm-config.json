{
  "primary_backend": "openrouter",
  "fallback_backend": "openrouter",
  "chat": {
    "backend": "openrouter",
    "max_tokens": 4096,
    "temperature": 0.7
  },
  "backends": {
    "local-ollama": {
      "type": "ollama",
      "endpoint": "http://localhost:11434",
      "model": "qwen2.5:14b",
      "timeout": 180,
      "cost_per_project": 0.0,
      "enabled": false
    },
    "remote-ollama": {
      "type": "ollama",
      "endpoint": "http://192.168.1.100:11434",
      "model": "qwen2.5:14b",
      "timeout": 180,
      "cost_per_project": 0.0,
      "enabled": false
    },
    "openai-mini": {
      "type": "openai",
      "endpoint": "https://api.openai.com/v1/chat/completions",
      "model": "gpt-4o-mini",
      "api_key_env": "OPENAI_API_KEY",
      "timeout": 180,
      "cost_per_project": 0.012,
      "enabled": false
    },
    "openai": {
      "type": "openai",
      "endpoint": "https://api.openai.com/v1/chat/completions",
      "model": "gpt-4o",
      "api_key_env": "OPENAI_API_KEY",
      "timeout": 180,
      "cost_per_project": 0.2,
      "enabled": false
    },
    "claude": {
      "type": "anthropic",
      "endpoint": "https://api.anthropic.com/v1/messages",
      "model": "claude-3-5-sonnet-latest",
      "api_key_env": "ANTHROPIC_API_KEY",
      "timeout": 180,
      "cost_per_project": 0.25,
      "enabled": false
    },
    "gemini-flash": {
      "type": "gemini",
      "endpoint": "https://generativelanguage.googleapis.com/v1beta/models/gemini-1.5-flash:generateContent",
      "model": "gemini-1.5-flash",
      "api_key_env": "GEMINI_API_KEY",
      "timeout": 180,
      "cost_per_project": 0.015
    },
    "gemini-flash-8b": {
      "type": "gemini",
      "endpoint": "https://generativelanguage.googleapis.com/v1beta/models/gemini-1.5-flash-8b:generateContent",
      "model": "gemini-1.5-flash-8b",
      "api_key_env": "GEMINI_API_KEY",
      "timeout": 180,
      "cost_per_project": 0.008
    },
    "gemini-pro": {
      "type": "gemini",
      "endpoint": "https://generativelanguage.googleapis.com/v1beta/models/gemini-1.5-pro:generateContent",
      "model": "gemini-1.5-pro",
      "api_key_env": "GEMINI_API_KEY",
      "timeout": 240,
      "cost_per_project": 0.08
    },
    "openrouter": {
      "type": "openrouter",
      "endpoint": "https://openrouter.ai/api/v1/chat/completions",
      "preset": "ai-editorial-assistant",
      "fallback_model": "google/gemini-2.5-flash",
      "api_key_env": "OPENROUTER_API_KEY",
      "timeout": 180,
      "cost_per_project": 0.02
    },
    "openrouter-big-brain": {
      "type": "openrouter",
      "endpoint": "https://openrouter.ai/api/v1/chat/completions",
      "preset": "ai-editorial-assistant-big-brain",
      "fallback_model": "google/gemini-2.5-flash",
      "api_key_env": "OPENROUTER_API_KEY",
      "timeout": 300,
      "cost_per_project": 0.1
    },
    "openrouter-cheapskate": {
      "type": "openrouter",
      "endpoint": "https://openrouter.ai/api/v1/chat/completions",
      "preset": "ai-editorial-assistant-cheapskate",
      "fallback_model": "google/gemini-2.5-flash",
      "api_key_env": "OPENROUTER_API_KEY",
      "timeout": 300,
      "cost_per_project": 0.0,
      "enabled": true
    }
  },
  "auto_select": {
    "enabled": true,
    "preference_order": [
      "openrouter"
    ],
    "check_interval_seconds": 5
  },
  "processing": {
    "check_queue_interval_seconds": 300,
    "batch_size": 4,
    "parallel_processing": false
  },
  "worker": {
    "max_concurrent_jobs": 3,
    "poll_interval_seconds": 5,
    "heartbeat_interval_seconds": 60
  },
  "phase_backends": {
    "analyst": "openrouter",
    "formatter": "openrouter-cheapskate",
    "seo": "openrouter-cheapskate",
    "manager": "openrouter-big-brain",
    "copy_editor": "openrouter",
    "chat": "openrouter"
  },
  "routing": {
    "tiers": [
      "openrouter-cheapskate",
      "openrouter",
      "openrouter-big-brain"
    ],
    "tier_labels": [
      "cheapskate",
      "default",
      "big-brain"
    ],
    "duration_thresholds": [
      {
        "max_minutes": 30,
        "tier": 0
      },
      {
        "max_minutes": 45,
        "tier": 1
      },
      {
        "max_minutes": null,
        "tier": 2
      }
    ],
    "phase_base_tiers": {
      "analyst": 0,
      "formatter": 0,
      "seo": 0,
      "manager": 2,
      "copy_editor": 2,
      "chat": 1
    },
    "escalation": {
      "enabled": true,
      "on_failure": true,
      "on_timeout": true,
      "timeout_seconds": 120,
      "max_retries_per_tier": 1
    }
  },
  "openrouter_presets": {
    "ai-editorial-assistant": {
      "description": "General use - balanced cost/quality",
      "models": [
        "google/gemini-3-flash-preview",
        "anthropic/claude-sonnet-4.5",
        "openai/gpt-5.1-codex-mini"
      ],
      "models_verified": "2026-01-29"
    },
    "ai-editorial-assistant-big-brain": {
      "description": "Complex reasoning - higher quality models",
      "models": [
        "google/gemini-3-pro-preview",
        "openai/gpt-5.1-codex",
        "anthropic/claude-opus-4.5"
      ],
      "models_verified": "2026-01-29"
    },
    "ai-editorial-assistant-cheapskate": {
      "description": "Free tier models for simple formatting tasks",
      "models": [
        "openai/gpt-oss-120b",
        "moonshotai/kimi-k2-0711",
        "nvidia/nemotron-nano-12b-v2-vl"
      ],
      "models_verified": "2026-01-29"
    }
  },
  "safety": {
    "run_cost_cap": 1.0,
    "max_cost_per_1k_tokens": 0.05,
    "model_allowlist": []
  }
}